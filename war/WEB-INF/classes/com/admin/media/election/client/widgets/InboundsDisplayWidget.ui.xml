<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		
		.name-column {
			float: left;
			width: 80px;
			padding-right: 5px;
			padding-left: 8px;
			color: #828282;
		}
		
		.type-column{
			float: left;
			width: 100px;
			padding-right: 5px;			
			color: #828282;
		}
		
		.poll-column {
			float: left;
			width: 110px;
			padding-right: 5px;	
			color: #828282;
		}
		
		.mobile-column{
			float: left;
			width: 100px;
			padding-right: 5px;
			color: #828282;
		}

		.message-column{
			float: left;
			width: 120px;
			padding-right: 5px;
			color: #828282;
		}
		
		.time-column{
			float: left;
			width: 100px;
			padding-right: 5px;
			color: #828282;
		}
		
		.approve-column{
			float: left;
			width: 55px;
			padding-right: 40px;
			color: #828282;
		}
		.actions-column {
			float: left;
			width: 40px;
		}
		
		.page-row {
			width: 789px;
			border-bottom: solid 1px #CCCCCC;
			border-left: solid 1px #CCCCCC;
			border-right: solid 1px #CCCCCC;
			color: #666;
		}
		
		.page-row-top {
			height: 20px;
			padding-top: 5px;
		}
		
		.clear {
			clear: both;
		}
		
		 .page-row .popdown-holder {
			width: 100%;
		}
		
		.page-row .popdown-holder .popdown {
			width: 100%;
			height: 80px;
			padding: 5px;
		}
		
		.actions-column a,.actions-column a:hover,.actions-column a:active,.actions-column a:visited
			{
			color: #0066CC;
		}
		
		.grayed {
			color: #999999;
		}
		
		 .add-measure-button {
			float: right;
		}
		
		.actionButton,.actionButtonSuppressed:active,.actionButtonSuppressed:focus,.actionButtonSuppressed:hover
			{
			border-bottom-color: #999;
			box-shadow: 0 1px 0 rgba(0, 0, 0, .1);
			-webkit-box-shadow: 0 1px 0 rgba(0, 0, 0, .1);
			cursor: pointer;
			display: -moz-inline-box;
			display: inline-block;
			font-size: 11px;
			font-weight: bold;
			line-height: normal !important;
			text-align: center;
			text-decoration: none;
			vertical-align: top;
			white-space: nowrap;
			background-color: #eee;
			background-image: url(images/bg_button.png);
			background-repeat: repeat;
			background-position: -10 0;
			border-top-width: 1px;
			border-right-width: 1px;
			border-bottom-width: 1px;
			border-left-width: 2px;
			border-top-style: solid;
			border-right-style: solid;
			border-bottom-style: solid;
			border-left-style: solid;
			border-top-color: #999;
			border-right-color: #999;
			border-left-color: #CC6600;
			padding-top: 3px;
			padding-right: 6px;
			padding-bottom: 3px;
			padding-left: 6px;
		}
		
		.actionButton,.actionButton {
			margin-left: 4px
		}
		
		.actionButton:hover {
			text-decoration: none
		}
		
		.actionButton:active,.actionButtonDepressed {
			background: #ddd;
			border-bottom-color: #999;
			box-shadow: 0 1px 0 rgba(0, 0, 0, .05);
			-webkit-box-shadow: 0 1px 0 rgba(0, 0, 0, .05);
			border-left-width: 2px;
			border-left-style: solid;
			border-left-color: #CC6600;
		}
		
		.actionButton .actionButtonText,.actionButton input {
			background: none;
			border: 0;
			color: #333;
			cursor: pointer;
			display: -moz-inline-box;
			display: inline-block;
			font-family: 'Lucida Grande', Tahoma, Verdana, Arial, sans-serif;
			font-size: 11px;
			font-weight: bold;
			margin: 0;
			outline: none;
			padding: 1px 0 2px;
			white-space: nowrap
		}
	
	.move-up-small{
		position: relative;
		top: -25px;
	}
	</ui:style>
	<g:HTMLPanel>
	<div ui:field="div1" class="{style.page-row-top}">
		<div class="{style.name-column}" ui:field="nameField">Frank</div>
		<div class="{style.mobile-column}" ui:field="msisdnField">+233244200000</div>
		<div class="{style.type-column}" ui:field="typeField">Paliamentary</div>
 		<div class="{style.message-column}" ui:field="messageField">NPP:2345 NDC:1456 PNC:345</div>
 		<div class="{style.time-column}" ui:field="timeField">12-06-12 16:02.45</div>
 		<div class="{style.poll-column}" ui:field="pollField">Polling Station A</div>
 		<div class="{style.approve-column}" ui:field="approveField"></div>
	<!-- 
					<g:HTML ui:field="nameField" styleName="{style.name-column}"></g:HTML>
					<g:HTML ui:field="genderField" styleName="{style.abbriv-column}"></g:HTML>
					<g:Label ui:field="ageField" styleName="{style.symbol-column}">&nbsp;</g:Label>
					<g:Label ui:field="descAliasField" styleName="{style.desc-column}">&nbsp;</g:Label> -->
					
				<div class="{style.actions-column}">
					<g:Anchor ui:field="detailsLink" text="details"/>
				</div>
			</div>
			<div class="{style.clear}"></div>
			<div class="{style.move-up-small}"></div>
	</g:HTMLPanel>
</ui:UiBinder>  