<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		.main{
			margin: 5px 0px 5px 0px;
		}
		
		.page-row-top {
			height: 20px;
			padding-top: 5px;
		}
		
		.color-column {
			float: left;
			width: 13px;
			height: 13px;
			margin-right: 35px;
			margin-left: 8px;
			margin-top: 4px;
		}
		
		.name-column {
			float: left;
			width: 180px;
			padding-right: 35px;
			padding-left: 8px;
			color: #828282;
		}
		
		.logo-column{
			float: left;
			width: 30px;
			height: 30px;
			padding-right: 123px;
		}
		
		.actions-column {
			float: right;
			width: 70px;
			text-align: right;
			padding-right: 27px;
		}
		
		.page-row {
			width: 789px;
			border-bottom: solid 1px #CCCCCC;
			border-left: solid 1px #CCCCCC;
			border-right: solid 1px #CCCCCC;
			color: #666;
		}
		
		.clear {
			clear: both;
		}
		
		.move-up-small{
			position: relative;
			top: -25px;
		}
	</ui:style>
	<g:HTMLPanel styleName="{style.main}">
<!-- 	<div ui:field="div1" class="{style.page-row-top}">
 -->		
 		<div class="{style.color-column}" ui:field="colorField"/>
 		<div class="{style.name-column}" ui:field="nameField">Name</div>
		<g:Image styleName="{style.logo-column}" ui:field="logoField"/>
	
		<div class="{style.actions-column}">
			<g:Anchor ui:field="editLink" text="edit"/>
<!-- 			 | <g:Anchor ui:field="deleteLink" text="delete"/>
 -->		</div>
<!-- 	</div>
 -->		<div class="{style.clear}"></div>
		<div class="{style.move-up-small}"></div>
	</g:HTMLPanel>
</ui:UiBinder> 